{
  "job": {
    "setting": {
      "speed": {
        "channel": 2
      }
    },
    "content": [
      {
        "reader": {
          "name": "ftpreader",
          "parameter": {
            "protocol": "sftp",
            "host": "sftp.example.com",
            "port": 22,
            "username": "testuser",
            "password": "testpass",
            "timeout": 60000,
            "path": "/home/user/data/*.csv",
            "maxTraversalLevel": 5,
            "column": ["*"],
            "fieldDelimiter": ",",
            "encoding": "UTF-8",
            "compress": "gzip",
            "skipHeader": true,
            "nullFormat": ""
          }
        },
        "writer": {
          "name": "ftpwriter",
          "parameter": {
            "protocol": "sftp",
            "host": "sftp.example.com",
            "port": 22,
            "username": "testuser",
            "password": "testpass",
            "timeout": 60000,
            "path": "/home/user/output",
            "fileName": "result",
            "suffix": "csv",
            "writeMode": "append",
            "fieldDelimiter": ",",
            "encoding": "UTF-8",
            "compress": "gzip",
            "nullFormat": "",
            "dateFormat": "2006-01-02 15:04:05"
          }
        }
      }
    ]
  }
}